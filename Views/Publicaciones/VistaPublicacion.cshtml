<div class="FondoGlass-3 my-5">


<input type="hidden" id="PublicacionID" value=@ViewBag.publicacionID></input>

<div class="row">
        @* carosel de imagenes *@
    <div class="col-12 col-md-8">
        <div id="carouselExampleFade" class="carousel slide my-3" data-bs-ride="carousel" data-bs-touch="true" data-bs-interval="false">
            <div class="carousel-inner" id="Lista_imagenes">
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleFade"
                data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleFade"
                data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>
    </div>
        @* datos principales*@
    <div class="col-12 col-md-4">
            @* titulo *@
            <h2 id=titulo></h2>
            @* tags *@
            @* <h3 id=tags>tags</h3> *@
            <spam style="display: none;" id="tags" class="tag"></spam>
            @* fecha *@
            <h5 class="mt-2" id=fecha>fecha</h5>
             @* usuario *@
            <div class="div-usuario">
                <span>
                <p id=User-name><svg xmlns="http://www.w3.org/2000/svg" height="3em" viewBox="0 0 512 512"><path d="M399 384.2C376.9 345.8 335.4 320 288 320H224c-47.4 0-88.9 25.8-111 64.2c35.2 39.2 86.2 63.8 143 63.8s107.8-24.7 143-63.8zM0 256a256 256 0 1 1 512 0A256 256 0 1 1 0 256zm256 16a72 72 0 1 0 0-144 72 72 0 1 0 0 144z"/></svg>
Jorgito86</p>
                </span>
            </div>
    </div>

        @* descripcion *@
<h4 id="descripcion"></h4>

       


<div class="div-boton-centrado my-3">
    <button class="btn btn-success">SOLICITAR</button>
